<body>
  
    <h3>Room code:</h3>
     <div class="d-flex align-items-center gap-2 mb-4">
    <input type="text" class="form-control w-auto" id="roomCodeInput" value="<%= roomId %>" readonly>
    <button class="btn btn-outline-primary" id="copyBtn">Copy</button>
    </div>

<input type="file" id="fileInput" accept="video/*">



    <video
    id="my-player"
    class="video-js"
    controls
    preload="auto"
    width="640"
    height="360"
    poster=""
    data-setup='{}'>
    <source id="videoSource"></source>
